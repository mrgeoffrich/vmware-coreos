{
  // Use IntelliSense to learn about possible Node.js debug attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node2",
      "request": "launch",
      "name": "Launch Coreos Setup",
      "program": "${workspaceRoot}/build/src/index.js",
      "cwd": "${workspaceRoot}",
      "outFiles": ["${workspaceRoot}/build/**/*"],
      "args": ["coreos-setup","stable","DellDatastoreSSD","coreos"],
      "sourceMaps": true,
      "console": "integratedTerminal",
      "env": {
        "NODE_ENV" :"development"
      }
    },
    {
      "type": "node2",
      "request": "launch",
      "name": "Launch Environment Create",
      "program": "${workspaceRoot}/build/src/index.js",
      "cwd": "${workspaceRoot}",
      "outFiles": ["${workspaceRoot}/build/**/*"],
      "args": ["env-create","deploy_files/environment_coreos.json","deploy_files/subnet_home.json","deploy_files/vmware_home.json"],
      "sourceMaps": true,
      "console": "integratedTerminal",
      "env": {
        "NODE_ENV" :"development"
      }
    },
      {
      "type": "node2",
      "request": "launch",
      "name": "Launch Environment Destroy",
      "program": "${workspaceRoot}/build/src/index.js",
      "cwd": "${workspaceRoot}",
      "outFiles": ["${workspaceRoot}/build/**/*"],
      "args": ["env-destroy","deploy_files/environment_coreos.json"],
      "sourceMaps": true,
      "console": "integratedTerminal",
      "env": {
        "NODE_ENV" :"development"
      }
    },
      {
      "type": "node2",
      "request": "launch",
      "name": "Launch Environment Validate",
      "program": "${workspaceRoot}/build/src/index.js",
      "cwd": "${workspaceRoot}",
      "outFiles": ["${workspaceRoot}/build/**/*"],
      "args": ["env-validate","deploy_files/environment_coreos.json","deploy_files/subnet_home.json","deploy_files/validation.json"],
      "sourceMaps": true,
      "console": "integratedTerminal",
      "env": {
        "NODE_ENV" :"development"
      }
    },
    {
      "type": "node2",
      "request": "attach",
      "name": "Attach to Process",
      "port": 5858,
      "outFiles": ["${workspaceRoot}/build/**/*"],
      "sourceMaps": true
    }
  ]
}